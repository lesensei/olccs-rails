<%
p = result['_source']
time = Time.strptime(p['time'], '%Y%m%d%H%M%S').strftime('%H:%M:%S')
ua_or_login = p['info'][0..20]
class_display = 'ua'
if p['login'] != ''
   ua_or_login = p['login']
   class_display = 'login'
end
re = Regexp.new("(?<=\s|^|>|#)(?:(?:([0-2]?[0-9])\:([0-5][0-9])\:([0-5][0-9])|([0-2]?[0-9])([0-5][0-9])([0-5][0-9]))(?:[\:\^]([0-9]{1,2})|([¹²³]))?)|([0-2]?[0-9])\:([0-5][0-9])(?=\s|$|<)")
message = p['message'].gsub(re) {|s|
   "<span class='horloge_ref'>#{s}</span>"
}
display = p['login'].presence || p['info']
%>
{
    "id": <%=p['id']%>,
    "time":"<%=time%>",
    "info":<%=raw p['info'].to_json%>,
    "class_display":"<%=class_display%>",
    "ua_or_login":<%=raw ua_or_login.to_json%>,
    "message":<%=raw message.to_json%>
}